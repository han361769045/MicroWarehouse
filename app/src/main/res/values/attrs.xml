<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension|reference" />
        <attr name="border_color" format="color|reference" />
        <attr name="border_overlay" format="boolean" />
    </declare-styleable>

    <color name="default_ev_border_color">#CCCCCC</color>
    <dimen name="default_ev_border_width">5dp</dimen>
    <dimen name="default_ev_border_radius">3dp</dimen>
    <color name="default_ev_password_color">#CCCCCC</color>
    <dimen name="default_ev_password_width">10dp</dimen>
    <dimen name="default_ev_password_radius">3dp</dimen>
    <integer name="default_ev_password_length">6</integer>

    <dimen name="custom_ev_border_width">10dp</dimen>
    <dimen name="custom_ev_border_radius">10dp</dimen>
    <dimen name="custom_ev_password_width">10dp</dimen>


    <attr name="centered" format="boolean" />
    <attr name="selectedColor" format="color" />
    <attr name="strokeWidth" format="dimension" />
    <attr name="unselectedColor" format="color" />

</resources>
